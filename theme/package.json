{
  "name": "slidev-theme-max",
  "version": "1.1.2",
  "author": "Kevin Delfour<kevin.delfour@maxds.fr>",
  "description": "A Max DS theme for Slidev",
  "keywords": [
    "slidev-theme",
    "slidev",
    "max"
  ],
  "license": "MIT",
  "private": false,
  "slidev": {
    "colorSchema": "both",
    "highlighter": "all"
  },
  "scripts": {
    "dev": "slidev example.md --open",
    "build": "slidev build example.md && cp -r _redirects ./dist/_redirects",
    "export": "slidev export example.md",
    "screenshot": "slidev export example.md --format png"
  },
  "dependencies": {
    "@slidev/cli": "^0.48.5",
    "@slidev/client": "^0.48.5",
    "@slidev/theme-default": "^0.25.0"
  },
  "devDependencies": {
    "playwright-chromium": "^1.42.1"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
